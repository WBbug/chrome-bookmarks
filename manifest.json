{
  "name": "chrome-Bookmarks",
  "version": "1.0",
  "description": "Save where you read",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["https://*/*"],
  "permissions": [
    "contextMenus",
    "tabs",
    "bookmarks",
    "storage",
    "scripting",
    "activeTab"
  ]
}
